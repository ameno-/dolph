# OpenAI API Key (required for chat mode)
OPENAI_API_KEY=sk-...

# MySQL Connection (set by setup-db.ts)
MYSQL_URL=mysql://root:testpass123@localhost:3306/testdb

# OR individual variables
# MYSQL_HOST=localhost
# MYSQL_PORT=3306
# MYSQL_USER=root
# MYSQL_PASS=testpass123
# MYSQL_DB=testdb

# Security
MYSQL_ALLOW_WRITE=false
MYSQL_ROW_LIMIT=1000

# Agent
AGENT_MODEL=gpt-4o-mini
